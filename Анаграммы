str1 = input("Введите первую строку: ")
str2 = input("Введите вторую строку: ")
str1 = str1.replace(' ', '')
str2 = str2.replace(' ', '')
str1 = str1.lower()
str2 = str2.lower()
set1 = set(str1)
set2 = set(str2)
if set1 == set2:
    print("Строки являются анаграммами")
else:
    print("Строки не являются анаграммами")
